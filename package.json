{
  "name": "live_project",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "server": "nodemon ./backend/server.js",
    "client": "npm --prefix ./frontend/vite-project run dev",
    "build": "npm --prefix ./frontend/vite-project install && npm --prefix ./frontend/vite-project run build && rm -rf ./dist && mv frontend/vite-project/dist ./dist",
    "seed": "node backend/seeds/runSeeds.js"
  },
  "devDependencies": {
    "@vitejs/plugin-react": "^4.6.0",
    "concurrently": "^8.2.2",
    "nodemon": "^3.1.10",
    "vite": "^6.3.5"
  },
  "dependencies": {
    "axios": "^1.10.0",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "react-router-dom": "^7.6.2"
  }
}